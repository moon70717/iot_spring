<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">
	<bean id="ex" class="com.iot.spring.Excute">
		<constructor-arg ref="developer"></constructor-arg>
		<property name="worker">
			<ref bean="deign" />
		</property>

	</bean>
	
	<context:component-scan base-package="com.iot.spring.dev">
	</context:component-scan>
	
	<bean id="developer" class="com.iot.spring.dev.Developer"></bean>
	<bean id="deign" class="com.iot.spring.dev.Designer"></bean>
</beans>
 